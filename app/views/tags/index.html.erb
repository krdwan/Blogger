<h1>All Tags</h1>

<ul id="articles">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag)%>
      <%= link_to "Delete", tag_path(tag), method: :delete %>
      <!--#class: 'article_title',id:"article_#{article.id}" -->
    </li>
  <% end %>
</ul>

